{
    "about": {
        {% for key, value in about.items() %}
            {{ key | tojson }} : {{ value | tojson }}{{ "," if not loop.last }}
        {% endfor %}
    },
    "dataResources": [
        {
            "resID": "learningData",
            "resPath": "tables/learningData.csv",
            "resType": "table",
            "resFormat": [
                "text/csv"
            ],
            "isCollection": false,
            "columns": [
                {
                    "colIndex": 0,
                    "colName": "d3mIndex",
                    "colType": "integer",
                    "role": [
                        "index"
                    ]
                },
                {% for column in columns %}
                {
                    "colIndex": {{ loop.index | tojson }},
                    "colName": {{ column.name | tojson }},
                    "colType": {{ column.type | tojson }},
                    "colDescription": {{column.description | tojson}},
                    "role": [
                        "attribute"
                    ]
                }{{ "," if not loop.last }}
                {% endfor %}
            ]
        }
    ]
}